# Пример файла конфигурации для Pupervisor
# Скопируйте этот файл в .env и настройте под свои нужды

SERVER_ADDRESS=:8080
LOG_LEVEL=info
DEBUG=false

# Настройки базы данных (опционально)
# DATABASE_URL=postgres://user:password@localhost:5432/pupervisor

# Настройки Redis (опционально)
# REDIS_URL=localhost:6379

# Настройки внешнего API (опционально)
# API_KEY=your_api_key_here
# API_URL=https://api.example.com